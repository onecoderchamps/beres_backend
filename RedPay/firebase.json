{
  "hosting": {
    "site": "indonesiamineclosure-register",
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/base.txt/",
        "destination": "/base.txt"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
